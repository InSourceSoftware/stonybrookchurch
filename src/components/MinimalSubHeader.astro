---
export interface Props {
  title: string,
  image: string,
  parents?: Parent[]
}

interface Parent {
  title: string,
  url: string
}

const { title, image } = Astro.props;
---

<div class="andro_subheader style-3 bg-cover bg-center bg-norepeat dark-overlay dark-overlay-2" style={`background-image: url(${image || '/assets/img/subheader-7.jpg'})`}>
  <div class="container">
    <h1>{title}</h1>
  </div>
</div>