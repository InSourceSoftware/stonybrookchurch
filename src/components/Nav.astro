---
import { NAVIGATION } from '../consts';
---
<ul class="andro_header-navigation-left">
  {NAVIGATION.map((link) => (
    <li class={link.dropdown ? 'has-children' : ''}>
      <a href={link.url}>{link.title}</a>
      {link.dropdown && (
        <ul>
          {link.dropdown.map((_link) => (
            <li>
              <a href={_link.url}>{_link.title}</a>
            </li>
          ))}
        </ul>
      )}
    </li>
  ))}
</ul>